<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.liurongchan.qinshui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/qinshui"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.liurongchan.qinshui.Load_Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.liurongchan.qinshui.Secondary_Forum_Activity" />
        <activity android:name="org.liurongchan.qinshui.Main_Forum_Activity" />
        <activity android:name="org.liurongchan.qinshui.Login_Activity" />
        <activity android:name="org.liurongchan.qinshui.Posts_Activity" />
        <activity android:name="org.liurongchan.qinshui.Post_Content_Activity" />
    </application>

</manifest>